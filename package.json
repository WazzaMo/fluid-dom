{
  "name": "fluid-dom",
  "version": "1.1.0",
  "description": "A fluid DOM (DSL style) library.",
  "main": "./lib/fluid-dom.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "run-s build-amd build-commonjs",
    "build-amd": "tsc -m amd --outFile fluid-dom/fluid-dom-amd.js",
    "build-commonjs": "tsc -m commonjs --outDir fluid-dom/commonjs",
    "typescript": "tsc",
    "build-dev": "parcel build -d dist --no-minify --target browser --global fluid -o fluid-dom-dev.js lib/fluid-dom.js",
    "build": "parcel build -d dist --target browser --global fluid -o fluid-dom.js lib/fluid-dom.js"
  },
  "files": [
    "*.js"
  ],
  "keywords": [
    "DOM",
    "web",
    "html"
  ],
  "author": "Warwick Molloy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WazzaMo/fluid-dom.git"
  },
  "homepage": "https://wazzamo.github.io/fluid-dom/",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "requirejs": "^2.3.6"
  }
}
