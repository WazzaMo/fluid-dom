{
  "name": "fluid-dom",
  "version": "1.2.0",
  "description": "A fluid DOM (DSL style) library.",
  "browser": "./fluid-dom.bundle.js",
  "main": "./fluid-dom.commonjs.js",
  "scripts": {
    "test": "run-s dist test-message",
    "test-message": "echo \"Open the HTML files in the tests directory to run the tests.\" ",
    "dist": "npm run typescript && npm run build-dev && npm run build && npm run post-dist",
    "typescript": "tsc",
    "build": "run-s build-bundle build-commonjs build-commonjs-mock",
    "build-dev": "run-s build-bundle-dev build-commonjs-dev",
    "build-bundle": "rollup -c rollup.config-minify.js --format iife --file fluid-dom.bundle.js",
    "build-commonjs": "rollup -c rollup.config-minify.js --format cjs --file fluid-dom.commonjs.js",
    "build-bundle-dev": "rollup -c rollup.config-dev.js --format iife --file fluid-dom.bundle.dev.js",
    "build-commonjs-dev": "rollup -c rollup.config-dev.js --format cjs --file fluid-dom.commonjs.dev.js",
    "build-commonjs-mock": "rollup -c rollup.config-mock.js --format cjs --file fluid-dom.commonjs.mock.js",
    "type-commonjs-mock": "dts-bundle-generator -o fluid-dom.commonjs.mock.d.ts src/mock-document.ts",
    "post-dist": "node post-dist.js",
    "doc": "typedoc --exclude node_modules/ --theme markdown --name \"Fluid-dom\" --out docs/api ./src/"
  },
  "files": [
    "fluid-dom*.js",
    "docs/**",
    "js/*.ts"
  ],
  "directories": {
    "doc": "docs",
    "live": "docs/live"
  },
  "types": [
    "type/src/*.d.ts"
  ],
  "keywords": [
    "DOM",
    "browser",
    "html",
    "functional",
    "LINQ",
    "fluid",
    "dsl"
  ],
  "author": "Warwick Molloy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WazzaMo/fluid-dom.git"
  },
  "homepage": "https://wazzamo.github.io/fluid-dom/",
  "devDependencies": {
    "dts-bundle-generator": "^1.6.1",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.65.2",
    "rollup-plugin-terser": "^2.0.2",
    "typedoc": "^0.13.0",
    "typedoc-plugin-markdown": "^1.1.18",
    "typescript": "^3.0.3"
  },
  "dependencies": {}
}
