{
  "name": "fluid-dom",
  "version": "1.1.0",
  "description": "A fluid DOM (DSL style) library.",
  "browser": "./dist/fluid-dom.js",
  "scripts": {
    "test": "run-s dist test-message",
    "test-message": "echo \"Open the HTML files in the tests directory to run the tests.\" ",
    "dist": "run-s typescript build-dev build",
    "typescript": "tsc",
    "build-dev": "parcel build -d dist --no-minify --target browser --global fluid -o fluid-dom-dev.js lib/fluid-dom-dev.js",
    "build": "parcel build -d dist --target browser --global fluid -o fluid-dom.js"
  },
  "files": [
    "dist/*.js"
  ],
  "types": [
    "lib/*.ts"
  ],
  "keywords": [
    "DOM",
    "web",
    "html"
  ],
  "author": "Warwick Molloy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WazzaMo/fluid-dom.git"
  },
  "homepage": "https://wazzamo.github.io/fluid-dom/",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7",
    "typescript": "^3.0.3"
  },
  "dependencies": {}
}
